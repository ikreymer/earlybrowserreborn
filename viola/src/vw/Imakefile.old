# Imakefile
# 
# The Motif version of violaWWW browser
# 
# Make sure that you first BUILD viola binaries (in ../src)
#

CC            = cc

LOCAL_LIBRARIES = ../libXPM/libXpm.a -lXm $(XTOOLLIB) $(XMULIB) $(XLIB)

SYS_LIBRARIES = -lm

TOP = 
LDCOMBINEFLAGS = 

#ifdef SunArchitecture
CDEBUGFLAGS   = -g

CCOPTIONS     = -Bstatic
#CCOPTIONS    = -Wall

#special case for our development machine
#ifdef NEED_LDL
CDEBUGFLAGS   = -g -ldl
#endif

WWW_BIN       = /usr/tmp/Library/sun4
#endif

#ifdef UltrixArchitecture
CDEBUGFLAGS   = -Wf,-XNl10000
WWW_BIN       = /usr/tmp/Library/decstation
#endif

CCOPTIONS     = -Bstatic -D_NO_PROTO -I/usr/include/X11 -I../viola -I../libXPM -I../libXPA/src -I../libWWW/src

#WWW_BIN       = /usr/tmp/Library/sun4
WWW_BIN       = ../libWWW/Library/Implementation

XPM_BIN       = ../libXPM

XPA_BIN       = ../libXPA/src

GIF_BIN       = ../libGIF

STG_BIN       = ../libStyle

